pub type Future<T>
pub type LimitError
pub type Duration

// タイムアウトとリソース制限
fn with_async_limits<T>(limits: AsyncLimits, future: Future<T>) -> Future<Result<T, LimitError>>

pub type AsyncLimits = {
  execution_timeout: Option<Duration>,
  memory_limit: Option<usize>,
  concurrent_tasks_limit: Option<usize>,
}
